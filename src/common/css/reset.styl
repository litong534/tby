html
  height: 100%;
  body
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
    font-family: 'Microsoft Yahei';
    a, a:hover, a:visited
      text-decoration: none;
    button, select
      border-style: none;
    button
      cursor: pointer;
      &:focus
        outline: none;
    ul
      padding: 0;
      margin: 0;
    li
      list-style: none;
